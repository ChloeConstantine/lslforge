            class="lslforge.wizards.LslForgeProjectWizard"
            finalPerspective="lslforge.LslForgePerspective"
            icon="icons/newlprj_wiz.gif"
            id="lslforge.LslForgeProjectWizard"
            name="LSLForge Project"
            preferredPerspectives="lslforge.LslForgePerspective"
            project="true">
         <description>
            Create a new LSLForge Project.  A LSLForge Project allows you to edit and test LSL code.
         </description>
      </wizard>
      <primaryWizard
            id="lslforge.LslForgeProjectWizard">
      </primaryWizard>
      <wizard
            category="lslforge.LslForge"
            class="lslforge.wizards.NewTestWizard"
            icon="icons/obj16/lslforge-test.gif"
            id="lslforge.newTestWizard"
            name="New LSLForge Test"
            preferredPerspectives="lslforge.LslForgePerspective">
      </wizard>
      <wizard
            category="lslforge.LslForge"
            class="lslforge.wizards.NewModuleWizard"
            icon="icons/obj16/lslforge.gif"
            id="lslforge.module_wizard"
            name="New LSLForge Module"
            preferredPerspectives="lslforge.LslForgePerspective">
      </wizard>
      <wizard
            category="lslforge.LslForge"
            class="lslforge.wizards.NewScriptWizard"
            icon="icons/obj16/lslforge.gif"
            id="lslforge.script_wizard"
            name="New LSLForge Script"
            preferredPerspectives="lslforge.LslForgePerspective">
      </wizard>
      <wizard
            category="lslforge.LslForge"
            class="lslforge.wizards.NewSimProjectWizard"
            icon="icons/obj16/lslforge-sim.gif"
            id="lslforge.newSimProjectWizard"
            name="New Sim Project File"
            preferredPerspectives="lslforge.LslForgePerspective">
      </wizard>
      <wizard
            category="lslforge.LslForge"
            class="lslforge.wizards.NewSimEventHandler"
            icon="icons/obj16/lslforge.gif"
            id="lslforge.newSimEventHandlerWizard"
            name="Sim Event Handler"
            preferredPerspectives="lslforge.LslForgePerspective">
      </wizard>
      <wizard
            category="lslforge.LslForge"
            class="lslforge.wizards.NewAvEventHandler"
            icon="icons/obj16/lslforge.gif"
            id="lslforge.newAvEventHandlerWizard"
            name="Avatar Event Handler"
            preferredPerspectives="lslforge.LslForgePerspective">
      </wizard>
      <wizard
            category="lslforge.lslforge.examples"
            class="lslforge.wizards.NewHttpRpcSampleWizard"
            icon="icons/obj16/lslforge.gif"
            id="lslforge.HttpRpcExample"
            name="HTTP/RPC Example"
            preferredPerspectives="lslforge.LslForgePerspective">
      </wizard>
      <wizard
            category="lslforge.lslforge.examples"
            class="lslforge.wizards.NewDialogSampleWizard"
            icon="icons/obj16/lslforge.gif"
            id="lslforge.DialogExample"
            name="Dialog Example"
            preferredPerspectives="lslforge.LslForgePerspective">
      </wizard>
      <category
            id="lslforge.lslforge.examples"
            name="LSLForge Examples">
      </category>
      <wizard
            category="lslforge.lslforge.examples"
            class="lslforge.wizards.NewHttpRequestSampleWizard"
            icon="icons/obj16/lslforge.gif"
            id="lslforge.HttpRequestExample"
            name="HTTP Request Example"
            preferredPerspectives="lslforge.LslForgePerspective">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.ide.projectNatureImages">
      <image
            icon="$nl$/icons/lsl_ovr.gif"
            id="lslforge.natureImage"
            natureId="lslforge.lslForgeNature">
      </image>
   </extension>
   <extension
         point="org.eclipse.ui.decorators">
      <decorator
            adaptable="true"
            class="lslforge.decorators.ErrorDecorator"
            id="lslforge.decorators.Error"
            label="Error Decorator"
            lightweight="true"
            location="BOTTOM_LEFT"
            state="true">
         <enablement>
            <and>
               <objectClass
                     name="org.eclipse.core.resources.IResource">
               </objectClass>
               <or>
                  <objectClass
                        name="org.eclipse.core.resources.IFolder">
                  </objectClass>
                  <objectClass
                        name="org.eclipse.core.resources.IFile">
                  </objectClass>
               </or>
            </and>
         </enablement>
      </decorator>
   </extension>
   <extension
         id="problem"
         name="LSLForge Problem"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <persistent
            value="false">
      </persistent>
   </extension>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="lslforge.launching.TestLaunchDelegate"
            id="lslforge.testLaunch"
            modes="run, debug"
            name="Run LSL Unit Test">
      </launchConfigurationType>
      <launchConfigurationType
            delegate="lslforge.launching.SimLaunchDelegate"
            id="lslforge.simLaunch"
            modes="run,debug"
            name="Run LSL Simulator">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="lslforge.launching.TestLaunchTabGroup"
            id="lslforge.testLaunchTabGroup"
            type="lslforge.testLaunch">
      </launchConfigurationTabGroup>
      <launchConfigurationTabGroup
            class="lslforge.launching.SimLaunchTabGroup"
            id="lslforge.simLaunchTabGroup"
            type="lslforge.simLaunch">
      </launchConfigurationTabGroup>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTypeImages">
      <launchConfigurationTypeImage
            configTypeID="lslforge.testLaunch"
            icon="icons/launchlslforge.gif"
            id="lslforge.testLaunchImage">
      </launchConfigurationTypeImage>
      <launchConfigurationTypeImage
            configTypeID="lslforge.simLaunch"
            icon="icons/launchlslforge.gif"
            id="lslforge.simLaunchImage">
      </launchConfigurationTypeImage>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="lslforge.LslForgePerspectiveFactory"
            icon="icons/lplus.gif"
            id="lslforge.LslForgePerspective"
            name="LSLForge Perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="lslforge.testview.TestRunnerViewPart"
            icon="icons/lslforgetestrun.gif"
            id="lslforge.TestRunnerView"
            name="TestRunner">
      </view>
      <view
            allowMultiple="false"
            class="lslforge.simview.SimWatcherViewPart"
