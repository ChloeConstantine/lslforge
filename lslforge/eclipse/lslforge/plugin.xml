<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

    
   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="LSLForge Editor"
            icon="icons/obj16/lslforge.gif"
            extensions="lslp,lslm"
            contributorClass="lslforge.editor.LslForgeActionContributor"
            class="lslforge.editor.LslForgeEditor"
            id="lslforge.editor.LslForgeEditor">
      </editor>
      <editor
            class="lslforge.editor.TestEditor"
            extensions="lslt"
            icon="icons/obj16/lslforge-test.gif"
            id="lslforge.editor.LslTestEditor"
            name="LSLForge Test Editor">
      </editor>
      <editor
            class="lslforge.editor.SimEditor"
            extensions="simp"
            icon="icons/obj16/lslforge-sim.gif"
            id="lslforge.editor.SimEditor"
            name="LslForge Sim Editor">
      </editor>
   </extension>
   
   <extension
         id="LslForgeSetupParticipantName"
         name="%documentSetupParticipantName"
         point="org.eclipse.core.filebuffers.documentSetup">
      <participant
            extensions="lslp,lslm"
            class="lslforge.editor.LslForgeDocumentSetupParticipant">
      </participant>
   </extension>
   
<!-- =========================================================================== -->
<!-- Template Editor Example                                                     -->
<!-- =========================================================================== -->



   <extension
         point="org.eclipse.debug.ui.launchShortcuts">
      <shortcut
            class="lslforge.launching.LaunchLslTestShortcut"
            description="Launches a LSLForge Test"
            icon="icons/lslforgetestrun.gif"
            id="lslforge.LaunchLslTest"
            label="Launch Lsl Test"
            modes="run,debug">
         <contextualLaunch>
            <enablement>
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
                  <iterate
                        ifEmpty="false">
                     <adapt
                           type="lslforge.lsltest.LslTestSuite">
                     </adapt>
                  </iterate>
               </with></enablement>
         </contextualLaunch>
      </shortcut>
      <shortcut
            class="lslforge.launching.LaunchLslSimShortcut"
            icon="icons/launchlslforge.gif"
            id="lslforge.LaunchLslSim"
            label="Launch In Lsl Sim"
            modes="run, debug">
         <contextualLaunch>
            <enablement>
               <or>
                  <with
                        variable="selection">
                     <count
                           value="1">
                     </count>
                     <iterate
                           ifEmpty="false">
                        <or>
                           <adapt
                                 type="lslforge.LslForgeScript">
                           </adapt>
                           <adapt
                                 type="lslforge.sim.SimProject$WorldNode">
                           </adapt>
                        </or>
                     </iterate>
                  </with>
                  <instanceof
                        value="lslforge.editor.LslForgeEditor">
                  </instanceof>
               </or>
            </enablement>
         </contextualLaunch>
      </shortcut>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.eclipse.core.resources.IResource"
            class="lslforge.ResourceAdapterFactory">
         <adapter
               type="lslforge.LslForgeElement">
         </adapter>
         <adapter
               type="lslforge.LslDerivedScript">
         </adapter>
         <adapter
               type="lslforge.lsltest.LslTestSuite">
         </adapter>
         <adapter
               type="lslforge.LslForgeScript">
         </adapter>
         <adapter
               type="lslforge.sim.SimProject$WorldNode">
         </adapter>
      </factory>
      <factory
            adaptableType="lslforge.LslForgeElement"
            class="lslforge.LslForgeAdapterFactory">
         <adapter
               type="org.eclipse.core.resources.IResource">
         </adapter>
         <adapter
               type="lslforge.LslForgeScript">
         </adapter>
      </factory>
      <factory
            adaptableType="lslforge.LslDerivedScript"
            class="lslforge.LslDerivedScriptAdapterFactory">
         <adapter
               type="org.eclipse.core.resources.IResource">
         </adapter>
      </factory>
      <factory
            adaptableType="lslforge.lsltest.LslTestSuite"
            class="lslforge.lsltest.LslTestSuiteAdapterFactory">
         <adapter
               type="org.eclipse.core.resources.IResource">
         </adapter>
      </factory>
      <factory
            adaptableType="lslforge.LslForgeScript"
            class="lslforge.LslForgeAdapterFactory">
         <adapter
               type="lslforge.LslForgeElement">
         </adapter>
         <adapter
               type="org.eclipse.core.resources.IResource">
         </adapter>
      </factory>
      <factory
            adaptableType="lslforge.sim.SimProject$WorldNode"
            class="lslforge.sim.SimProjectAdapterFactory">
         <adapter
               type="org.eclipse.core.resources.IResource">
         </adapter>
      </factory>
      <factory
            adaptableType="org.eclipse.ui.part.FileEditorInput"
            class="lslforge.LslForgeAdapterFactory">
         <adapter
               type="lslforge.LslForgeScript">
         </adapter>
      </factory>
   </extension>
   <extension
         id="lslForgeNature"
         name="LSLForge Project"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="lslforge.LslProjectNature">
         </run>
      </runtime>
   </extension>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="lslforge.PropertyTester"
            id="lslforge.PropertyTester"
            namespace="lslforge"
            properties="isInLslProject"
            type="org.eclipse.core.runtime.IAdaptable">
      </propertyTester>
   </extension>
   <extension
